{
  "name": "gatsby-source-airtable-next",
  "version": "1.0.0",
  "author": "@davidpaulsson",
  "description": "Gatsby source plugin for Airtable, build for Gatsby v4 and beyond ðŸš€",
  "keywords": [
    "airtable",
    "gatsby",
    "gatsby-plugin"
  ],
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsc gatsby-node.ts --strict --noUnusedLocals --noUnusedParameters --noImplicitReturns --module \"commonjs\" --removeComments false --preserveConstEnums --skipLibCheck --esModuleInterop --sourceMap --target \"es2017\" --declaration --lib \"es2017\" --watch",
    "build": "tsc gatsby-node.ts --strict --noUnusedLocals --noUnusedParameters --noImplicitReturns --module \"commonjs\" --removeComments false --preserveConstEnums --skipLibCheck --esModuleInterop --sourceMap --target \"es2017\" --declaration --lib \"es2017\"",
    "postversion": "cp ../README.md ./README.md && cp ../CHANGELOG.md ./CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidpaulsson/gatsby-source-airtable-next.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidpaulsson/gatsby-source-airtable-next/issues"
  },
  "homepage": "https://github.com/davidpaulsson/gatsby-source-airtable-next#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "airtable": "^0.11.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0 || ^4.0.0"
  }
}
